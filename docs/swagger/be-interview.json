  "components": {
    "schemas": {
    },
    "parameters": {
    },
    "responses": {
      "NotFound": {
        "description": "Resources not found",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "code": {
                  "type": "number",
                  "example": 400
                },
                "message": {
                  "type": "string",
                  "example": "Resources not found"
                }
              }
            }
          }
        }
      },
      "IllegalInput": {
        "description": "Illegal input for operation."
      }
    },
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "description": "JWT Authorization header using the Bearer scheme.",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "paths": {
    "/analyze/file": {
      "post": {
        "description": "Analyze File",
        "tags": [
          "Virus Total"
        ],
        "parameters": [
          {
            "name": "x-apikey",
            "in": "header",
            "description": "API Key",
            "required": true,
            "schema": {
              "type": "string",
              "example": "8e87d9de16c437b9182e6a382276a96d15cb19fd34145a175f015260eb8ed8f2"
            }
          }
        ],
        "requestBody": {
          "description": "Payload",
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary"
                  }
                },
                "required": [
                  "file"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Response of the Request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "properties": {
                        "data": {
                          "properties": {
                            "id": {
                              "example": "NWFkMjdhZmYyN2RkNzY4MTg5YmU5YzAwYzdlN2EwYWY6MTY3ODAwMjc2NQ==",
                              "type": "string"
                            },
                            "links": {
                              "properties": {
                                "self": {
                                  "example": "https://www.virustotal.com/api/v3/analyses/NWFkMjdhZmYyN2RkNzY4MTg5YmU5YzAwYzdlN2EwYWY6MTY3ODAwMjc2NQ==",
                                  "type": "string"
                                }
                              },
                              "type": "object"
                            },
                            "type": {
                              "example": "analysis",
                              "type": "string"
                            }
                          },
                          "type": "object"
                        }
                      },
                      "type": "object"
                    },
                    "message": {
                      "example": "File uploaded successfully",
                      "type": "string"
                    },
                    "status": {
                      "example": true,
                      "type": "boolean"
                    }
                  },
                  "required": [
                    "data",
                    "message",
                    "status"
                  ]
                }
              }
            }
          }
        }
      }
    },
